{
  "compileOnSave": false,
  "compilerOptions": {
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2019",
    "baseUrl": ".",
    "allowSyntheticDefaultImports": true,
    "useUnknownInCatchVariables": false
  },
  "exclude": ["*"]
}
